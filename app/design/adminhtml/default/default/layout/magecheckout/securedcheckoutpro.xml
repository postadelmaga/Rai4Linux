<layout version="0.1.0">
    <adminhtml_system_config_edit>
        <reference name="head">
            <action method="addJs">
                <file>magecheckout/securedcheckoutpro/jquery/jquery-1.11.1.min.js</file>
            </action>
            <action method="addJs">
                <file>magecheckout/securedcheckoutpro/jquery/jquery-noconflict.js</file>
            </action>
            <action method="addJs">
                <script>magecheckout/securedcheckoutpro/jquery/jquery-ui.min.js</script>
            </action>
            <action method="addCss">
                <stylesheet>css/magecheckout/securedcheckoutpro/jquery-ui.min.css</stylesheet>
            </action>
            <action method="addCss">
                <stylesheet>css/magecheckout/securedcheckoutpro/field-order.css</stylesheet>
            </action>
        </reference>
    </adminhtml_system_config_edit>
    <adminhtml_sales_order_create_index>
        <reference name="optional_zip_countries">
            <action method="setTemplate">
                <template>magecheckout/securedcheckoutpro/directory/js/optional_zip_countries.phtml</template>
            </action>
        </reference>
    </adminhtml_sales_order_create_index>
    <adminhtml_customer_edit>
        <reference name="optional_zip_countries">
            <action method="setTemplate">
                <template>magecheckout/securedcheckoutpro/directory/js/optional_zip_countries.phtml</template>
            </action>
        </reference>
    </adminhtml_customer_edit>
    <!--Survey-->
    <adminhtml_sales_order_view>
        <reference name="order_item_extra_info">
            <block type="securedcheckoutpro/adminhtml_totals_order_view_tab_survey"
                   as="securedcheckoutpro_survey"/>
        </reference>
    </adminhtml_sales_order_view>
    <!--/Survey-->
</layout>